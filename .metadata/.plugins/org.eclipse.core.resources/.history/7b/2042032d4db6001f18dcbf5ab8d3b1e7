#include <stddef.h>
#include <stdbool.h>
#include "publisher.h"
#include "main.h"

static node_t* head = NULL;

void publish(node_t* node) {
	node->_l_ival = node->ival;
	node->notify = true;
	insert(head, node);
}

void notify_all() {

}
